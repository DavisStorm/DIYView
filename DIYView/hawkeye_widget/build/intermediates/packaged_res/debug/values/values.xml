<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="ruler_indicator_color">#E64D14</color>
    <color name="ruler_line_color">#9E9E9E</color>
    <color name="ruler_text_color">#909090</color>
    <color name="timeruler_showtime_bg_color">#000000</color>
    <color name="timeruler_showtime_text_color">#E6FFFFFF</color>
    <color name="txt_black">#2A2A2A</color>
    <dimen name="page_font_size">12sp</dimen>
    <dimen name="taper_chart_height">240dp</dimen>
    <dimen name="taper_chart_margin">12dp</dimen>
    <dimen name="taper_text_size">10sp</dimen>
    <dimen name="timerulerview_arrowheight">6dp</dimen>
    <dimen name="timerulerview_arrowheight_margin">1dp</dimen>
    <dimen name="timerulerview_contentheight">36dp</dimen>
    <dimen name="timerulerview_height">60dp</dimen>
    <dimen name="timerulerview_scaleheight">5dp</dimen>
    <dimen name="timerulerview_showtime_corner">2dp</dimen>
    <dimen name="timerulerview_showtime_marginbottom">1dp</dimen>
    <dimen name="timerulerview_showtime_textsize">14sp</dimen>
    <dimen name="timerulerview_showtimeheight">28dp</dimen>
    <dimen name="timerulerview_showtimewidth">48dp</dimen>
    <string name="app_name">CustomView</string>
    <string name="appbar_scrolling_view_behavior" translatable="false">com.google.android.material.appbar.AppBarLayout$ScrollingViewBehavior</string>
    <string name="title_animator_bmp_overturn">图片翻转动画</string>
    <string name="title_section_probar">SectionProBar</string>
    <declare-styleable name="CircleProgress">
        <!--是否开启抗锯齿-->
        <attr format="boolean" name="antiAlias"/>
        <!--圆弧起始角度，3点钟方向为0，小于0或大于360进行取余-->
        <attr format="float" name="startAngle"/>
        <!--圆弧度数-->
        <attr format="float" name="sweepAngle"/>
        <!--设置动画时间-->
        <attr format="integer" name="animTime"/>
        <!--设置内容的数值-->
        <attr format="float" name="circlemaxValue"/>
        <attr format="float" name="value"/>
        <!--设置内容的单位-->
        <attr format="string|reference" name="unit"/>
        <attr format="dimension" name="unitSize"/>
        <attr format="color|reference" name="unitColor"/>
        <!--绘制内容相应的提示语-->
        <attr format="string|reference" name="hint"/>
        <attr format="dimension" name="hintSize"/>
        <attr format="color|reference" name="hintColor"/>
        <!--精度，默认为0-->
        <attr format="integer" name="precision"/>
        <attr format="dimension" name="valueSize"/>
        <attr format="color|reference" name="valueColor"/>
        <!--背景圆弧颜色，默认白色-->
        <attr format="color|reference" name="bgArcColor"/>
        <!--圆弧宽度-->
        <attr format="dimension" name="arcWidth"/>
        <!--圆弧颜色-->
        <attr format="color|reference" name="arcColors"/>
        <!--文字的偏移量 相对于圆半径而言，默认三分之一 -->
        <attr format="float" name="textOffsetPercentInRadius"/>
        <!--背景圆弧宽度-->
        <attr format="dimension" name="bgArcWidth"/>
        <!--线条数-->
        <attr format="integer" name="dottedLineCount"/>
        <!--圆弧跟虚线之间的距离-->
        <attr format="integer" name="lineDistance"/>
        <!--线条宽度-->
        <attr format="dimension" name="dottedLineWidth"/>
        <!--进度条的前景色 起始颜色-->
        <attr format="color" name="foreStartColor"/>
        <!--进度条的前景色 结束颜色-->
        <attr format="color" name="foreEndColor"/>
    </declare-styleable>
    <declare-styleable name="CircleProgressBarView">

        <attr format="dimension" name="circleBgStrokeWidth"/>
        <attr format="dimension" name="progressStrokeWidth"/>

        <attr format="color" name="circleBgColor"/>
        <attr format="color" name="progressColor"/>

        <attr format="integer" name="circleAnimationDuration"/>

        <attr format="boolean" name="isDrawCenterProgressText"/>

        <attr format="color" name="centerProgressTextColor"/>
        <attr format="dimension" name="centerProgressTextSize"/>
        <attr format="color" name="targetNumColor"/>
        <attr format="dimension" name="targetNumSize"/>
        <attr format="color" name="targetTextColor"/>
        <attr format="integer" name="targetTextSize"/>
        <attr format="integer" name="lineWidthcircle"/>
        <attr format="color" name="lineColor"/>
        <attr format="string" name="target_text"/>

        <attr format="color" name="round_color"/>
        <attr format="color" name="round_progress_color"/>
        <attr format="dimension" name="round_width"/>
        <attr format="integer" name="max_"/>
        <attr format="boolean" name="text_isDisplayable"/>
        <attr name="style_">
            <enum name="STROKE" value="0"/>
            <enum name="FILL" value="1"/>
        </attr>

    </declare-styleable>
    <declare-styleable name="CircleView">
        <!--中间内容类型(文字|图标|二者都有)-->
        <attr format="enum" name="cv_center_style">
            <!--图标-->
            <enum name="icon" value="0"/>
            <!--文案-->
            <enum name="txt" value="1"/>
            <!--两行文案-->
            <enum name="double_txt" value="2"/>
            <!--二者都有-->
            <enum name="all" value="3"/>
        </attr>
        <!--中间图资源文件-->
        <attr format="reference" name="cv_center_bmp"/>
        <!--中间图文案-->
        <attr format="string" name="cv_center_txt"/>
        <!--中间图文案字体大小-->
        <attr format="dimension" name="cv_center_txt_size"/>
        <!--中间图文案字体颜色-->
        <attr format="color" name="cv_center_txt_color"/>
        <!--中间图文案字体两行文本间的间距-->
        <attr format="dimension" name="cv_cengter_txt_margin"/>
        <!-- 圆环宽度，默认值为 2dp -->
        <attr format="dimension" name="cv_boderW"/>
        <!-- 圆形进度条半径，默认值为 16dp -->
        <attr format="dimension" name="cv_radius"/>
    </declare-styleable>
    <declare-styleable name="GradientLine">
        <!-- 渐变曲线样式，默认值为 flat -->
        <attr format="enum" name="gl_mode">
            <enum name="flat" value="0"/>
            <enum name="up" value="1"/>
            <enum name="down" value="2"/>
        </attr>
        <!--曲线宽度-->
        <attr format="dimension" name="gl_line_width"/>
    </declare-styleable>
    <declare-styleable name="GradientLineMine">
        <attr format="integer" name="duration_mine"/>
    </declare-styleable>
    <declare-styleable name="GradientTextView">
        <attr format="color" name="originColor"/>
        <attr format="color" name="changeColor"/>
    </declare-styleable>
    <declare-styleable name="HalfCircleProView">
        <attr format="dimension" name="strokeWidth"/>
        <attr format="color" name="txtColor"/>
        <attr format="dimension" name="txtSize"/>
    </declare-styleable>
    <declare-styleable name="HorProBar">
        <attr format="color|reference" name="pro_color"/>
        <attr format="dimension" name="pro_width"/>
        <attr format="dimension" name="pro_height"/>
    </declare-styleable>
    <declare-styleable name="HorScrollSelectedView">
        <!--可见数目-->
        <attr format="integer" name="seeSize"/>
        <!--被选择文字的大小和颜色-->
        <attr format="float" name="selectedTextSize"/>
        <attr format="color" name="selectedTextColor"/>
        <!--未被选择文字的大小和颜色-->
        <attr format="float" name="hortextSize"/>
        <attr format="color|reference" name="textColor"/>
    </declare-styleable>
    <declare-styleable name="HorTaperChart">
        <!--y轴是否是百分制-->
        <attr format="boolean" name="htc_yaxis_percent"/>
        <!--文字大小-->
        <attr format="dimension" name="htc_axis_txtsize"/>
        <!--图形相交长度-->
        <attr format="dimension" name="htc_inner_space"/>
        <!--图表控制点的偏移量-->
        <attr format="dimension" name="htc_quad_x_offset"/>
        <!--是否绘制峰值顶尖值-->
        <attr format="boolean" name="htc_is_draw_topValue"/>
    </declare-styleable>
    <declare-styleable name="LegendRingView">
        <!--中间内容类型(文字|图标|二者都有)-->
        <attr format="enum" name="lrv_circle_center_style">
            <!--图标-->
            <enum name="icon" value="0"/>
            <!--文案-->
            <enum name="txt" value="1"/>
            <!--两行文案-->
            <enum name="double_txt" value="2"/>
            <!--二者都有-->
            <enum name="all" value="3"/>
        </attr>
        <!--圆环视图高度-->
        <attr format="dimension" name="lrv_circle_height"/>
        <!-- 圆环宽度，默认值为 2dp -->
        <attr format="dimension" name="lrv_circle_boderW"/>
        <!--圆环内间距-上，默认为0-->
        <attr format="dimension" name="lrv_circle_paddingTop"/>
        <!--圆环内间距-下，默认为0-->
        <attr format="dimension" name="lrv_circle_paddingBottom"/>
        <!--圆环中间图文案-->
        <attr format="string" name="lrv_circle_center_txt"/>
        <!--中间图文案字体大小-->
        <attr format="dimension" name="lrv_circle_center_txt_size"/>
        <!--中间图文案字体颜色-->
        <attr format="color" name="lrv_circle_center_txt_color"/>
        <!--图例列宽-->
        <attr format="dimension" name="lrv_legend_columnW"/>
        <!--图例行间距-->
        <attr format="dimension" name="lrv_legend_vertical_margin"/>
        <!--图例和圆环的间距，默认为0-->
        <attr format="dimension" name="lrv_legend_circle_margin"/>
        <!--图例文字大小-->
        <attr format="dimension" name="lrv_legend_font_size"/>
        <!--图例文字颜色-->
        <attr format="color" name="lrv_legend_font_color"/>
        <!--图例文案和圆点的间距-->
        <attr format="dimension" name="lrv_legend_labelAndPoint_margin"/>
        <!--图例每行第一个左侧偏移量-->
        <attr format="dimension" name="lrv_offset_left"/>
        <!--图例样式-->
        <attr format="enum" name="lrv_legend_style">
            <!--圆点-->
            <enum name="circle" value="0"/>
            <!--方点-->
            <enum name="square" value="1"/>
            <!--横线-->
            <enum name="line" value="2"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="LegendView">
        <!--图例列宽-->
        <attr format="dimension" name="lv_legend_columnW"/>
        <!--图例行间距-->
        <attr format="dimension" name="lv_legend_vertical_margin"/>
        <!--图例和圆环的间距，默认为0-->
        <attr format="dimension" name="lv_legend_circle_margin"/>
        <!--图例文字大小-->
        <attr format="dimension" name="lv_legend_font_size"/>
        <!--图例文字颜色-->
        <attr format="color" name="lv_legend_font_color"/>
        <!--图例文案和圆点的间距-->
        <attr format="dimension" name="lv_legend_labelAndPoint_margin"/>
        <!--图例每行第一个左侧偏移量-->
        <attr format="dimension" name="lv_legend_offset_left"/>
        <!--图例样式-->
        <attr format="enum" name="lv_legend_style">
            <!--圆点-->
            <enum name="circle" value="0"/>
            <!--方点-->
            <enum name="square" value="1"/>
            <!--横线-->
            <enum name="line" value="2"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="RingView">
        <!-- 进度条样式，默认值为 system -->
        <attr format="enum" name="rv_mode">
            <!-- 圆形进度条 -->
            <enum name="circle" value="0"/>
            <!-- 圆形进度条-多段 -->
            <enum name="more" value="1"/>
            <!-- 圆形进度条-客群 -->
            <enum name="group" value="2"/>
        </attr>
        <!--中间内容类型(文字|图标|二者都有)-->
        <attr format="enum" name="rv_center_style">
            <!--图标-->
            <enum name="icon" value="0"/>
            <!--文案-->
            <enum name="txt" value="1"/>
            <!--双行文字-->
            <enum name="double_txt" value="2"/>
            <!--二者都有-->
            <enum name="all" value="3"/>
        </attr>
        <!--中间图资源文件-->
        <attr format="reference" name="rv_center_bmp"/>
        <!--中间图文案-->
        <attr format="string" name="rv_center_txt"/>
        <!--中间图文案字体大小-->
        <attr format="dimension" name="rv_center_txt_size"/>
        <!--中间图文案字体颜色-->
        <attr format="color" name="rv_center_txt_color"/>
        <!--中间图文结合，两者之间的间距-->
        <attr format="dimension" name="rv_center_imgtxt_margin"/>
        <!-- 文字颜色，默认值为 #70A800 -->
        <attr format="reference|color" name="rv_textColor"/>
        <!-- 文字大小，默认值为 10sp -->
        <attr format="dimension" name="rv_textSize"/>
        <!-- 文字和进度条之间的间距，默认值为 4dp -->
        <attr format="dimension" name="rv_textMargin"/>
        <!-- 已完成进度的颜色，默认值为 #70A800 -->
        <attr format="reference|color" name="rv_reachedColor"/>
        <!-- 已完成进度的高度，默认值为 2dp -->
        <attr format="dimension" name="rv_reachedHeight"/>
        <!-- 未完成进度的颜色，默认值为 #CCCCCC -->
        <attr format="reference|color" name="rv_unReachedColor"/>
        <!-- 未完成进度的高度，默认值为 1dp -->
        <attr format="dimension" name="rv_unReachedHeight"/>
        <!-- 圆形进度条半径，默认值为 16dp -->
        <attr format="dimension" name="rv_radius"/>
        <!--边线是否可点击-->
        <attr format="boolean" name="rv_outer_enable"/>
        <!--是否需要分割-->
        <attr format="boolean" name="rv_need_cut"/>
    </declare-styleable>
    <declare-styleable name="RingViewMine">
<!--        名称文字-->
        <attr format="string" name="center_text"/>
<!--        中间图片-->
        <attr format="reference" name="center_bmp"/>
<!--        圆环半径-->
        <attr format="dimension" name="ringRadius"/>
<!--        圆环的宽度-->
        <attr format="dimension" name="ringWidth"/>
        <!--标线长度-->
        <attr format="dimension" name="biaoxianlength"/>
<!--        名称文字大小-->
        <attr format="dimension" name="name_textsize"/>
    </declare-styleable>
    <declare-styleable name="SectionProBar">
        <!-- 已完成进度的颜色，默认值为 #70A800 -->
        <attr format="reference|color" name="hbv_reachedColor"/>
        <!-- 已完成进度的高度，默认值为 2dp -->
        <attr format="dimension" name="hbv_reachedHeight"/>
        <!-- 未完成进度的颜色，默认值为 #CCCCCC -->
        <attr format="reference|color" name="hbv_unReachedColor"/>
        <!-- 未完成进度的高度，默认值为 1dp -->
        <attr format="dimension" name="hbv_unReachedHeight"/>
    </declare-styleable>
    <declare-styleable name="SectionProBarMine">
        <!-- 已完成进度的颜色，默认值为 #70A800 -->
        <attr format="reference|color" name="hbv_reachedColorMine"/>
        <!-- 已完成进度的高度，默认值为 2dp -->
        <attr format="dimension" name="hbv_reachedHeightMine"/>
        <!-- 未完成进度的颜色，默认值为 #CCCCCC -->
        <attr format="reference|color" name="hbv_unReachedColorMine"/>
        <!-- 未完成进度的高度，默认值为 1dp -->
        <attr format="dimension" name="hbv_unReachedHeightMine"/>
        <attr format="enum" name="hbv_style">
            <enum name="NORMAL" value="0"/>
            <enum name="GRADIENT" value="1"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="ShadowProBar">
        <attr format="color|reference" name="progress_color"/>
    </declare-styleable>
    <declare-styleable name="TaperChart">
        <!-- 图形样式，默认值为 system -->
        <attr format="enum" name="tc_mode">
            <!-- 2张图，2个label -->
            <enum name="first" value="0"/>
            <!-- 3张图，3个label -->
            <enum name="second" value="1"/>
            <!-- 4张图，4个label -->
            <enum name="third" value="2"/>
            <!-- 4张图，2个label -->
            <enum name="fourth" value="3"/>
            <!-- 6张图，3个label -->
            <enum name="fifth" value="4"/>
        </attr>
        <!--y轴是否是百分制-->
        <attr format="boolean" name="tc_yaxis_percent"/>
        <!--文字大小-->
        <attr format="dimension" name="tc_axis_txtsize"/>
        <!--图形相交长度-->
        <attr format="dimension" name="tc_inner_space"/>
        <!--图表控制点的偏移量-->
        <attr format="dimension" name="tc_quad_x_offset"/>
        <!--是否绘制峰值顶尖值-->
        <attr format="boolean" name="is_draw_topvalue"/>
    </declare-styleable>
    <declare-styleable name="TimeRulerView">
        <attr format="dimension|reference" name="textFontSize"/>
        <attr name="totalTimePerCell">
            <enum name="hour" value="24"/>
            <enum name="halfHour" value="48"/>
            <enum name="fifteenmin" value="96"/>
            <enum name="fivemin" value="288"/>
        </attr>
        <attr format="color|reference" name="textColors"/>
        <attr format="color|reference" name="scaleColor"/>
        <attr format="color|reference" name="topLineColor"/>
        <attr format="color|reference" name="bottomLineColor"/>
        <attr format="color|reference" name="middleLineColor"/>
        <attr format="color|reference" name="selectBackgroundColor"/>
        <attr format="dimension|reference" name="topLineStrokeWidth"/>
        <attr format="dimension|reference" name="bottomLineStrokeWidth"/>
        <attr format="dimension|reference" name="middleLineStrokeWidth"/>
        <attr format="dimension|reference" name="scaleLineStrokeWidth"/>
        <attr format="dimension|reference" name="widthPerScale"/>
        <attr format="dimension|reference" name="showtimeHeight"/>
        <attr format="dimension|reference" name="contentHeight"/>
    </declare-styleable>
</resources>