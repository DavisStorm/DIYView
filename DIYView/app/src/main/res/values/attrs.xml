<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="StatisticsView">
        <attr name="maxValue" format="integer"></attr>
        <attr name="dividerCount" format="integer"></attr>
        <attr name="title" format="string"></attr>
        <attr name="lineColor" format="color"></attr>
        <attr name="textColor" format="color"></attr>
        <attr name="pathColor" format="color"></attr>
    </declare-styleable>
    <declare-styleable name="MeasureView">
        <attr name="text" format="string"></attr>
        <attr name="textSize" format="integer"></attr>
    </declare-styleable>
    <declare-styleable name="DragImageView">
        <attr name="drawable" format="reference"></attr>
    </declare-styleable>
    <declare-styleable name="MyDragImageView">
        <attr name="mydrawable" format="reference"/>
    </declare-styleable>
    <declare-styleable name="CircleDotProgressBar">
        <attr name="progressMax" format="integer"/>
        <attr name="dotColor" format="color"/>
        <attr name="dotBgColor" format="color"/>
        <attr name="showMode" format="integer"/>
        <attr name="percentTextSize" format="dimension"/>
        <attr name="percentTextColor" format="integer"/>
        <attr name="unitText" format="string"/>
        <attr name="unitTextSize" format="dimension"/>
        <attr name="unitTextColor" format="integer"/>
        <attr name="unitTextAlignMode" format="integer"/>
        <attr name="buttonText" format="string"/>
        <attr name="buttonTextSize" format="dimension"/>
        <attr name="buttonTextColor" format="integer"/>
        <attr name="buttonBgColor" format="integer"/>
        <attr name="buttonClickColor" format="integer"/>
        <attr name="buttonClickBgColor" format="integer"/>
        <attr name="buttonTopOffset" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="CircleDotProgressBardiy">
        <attr name="lightDotColor" format="color"/>
        <attr name="dimDotColor" format="color"/>
        <attr name="maxProgress" format="integer"/>
        <attr name="progress" format="integer"/>
        <attr name="percentTextSizediy" format="dimension"/>
        <attr name="percentTextColordiy" format="color"/>
        <attr name="unitTextSizediy" format="dimension"/>
        <attr name="unitTextColordiy" format="color"/>
        <attr name="unitTextdiy" format="string"/>
        <attr name="btnText" format="string"/>
        <attr name="btnTextSize" format="dimension"/>
        <attr name="btnTextColor" format="color"/>
        <attr name="btnYspace" format="dimension"/>
        <attr name="btnBgColor" format="color"/>
    </declare-styleable>
    <declare-styleable name="ViewPager">
    </declare-styleable>
    <declare-styleable name="LitePager">
        <attr name="orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>

        <attr name="flingDuration" format="integer" />
        <attr name="bottomScale" format="float" />
        <attr name="bottomAlpha" format="float" />
        <attr name="middleScale" format="float" />
        <attr name="middleAlpha" format="float" />
        <attr name="topScale" format="float" />
        <attr name="topAlpha" format="float" />
        <attr name="autoScroll" format="boolean" />
        <attr name="autoScrollInterval" format="integer" />
        <attr name="autoScrollOrientation" format="enum">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
            <enum name="up" value="0" />
            <enum name="down" value="1" />
        </attr>
    </declare-styleable>
    <declare-styleable name="UniverseStarsView">
        <attr name="imgPadding" format="dimension"/>
        <attr name="dotSize" format="dimension"/>
        <attr name="dotDuration" format="integer"/>
        <attr name="myDotColor" format="color"/>
        <attr name="myDotMaxColor" format="integer"/>
        <attr name="dotNum" format="integer"/>
        <attr name="maxOffset" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="RoundRecImageView">
        <attr name="circle" format="boolean" />
        <attr name="radius" format="dimension" />
        <attr name="border_width" />
        <attr name="border_color" />
    </declare-styleable>
    <declare-styleable name="SmoothPagingView">
        <attr name="img1" format="reference"/>
        <attr name="img2" format="reference"/>
        <attr name="img3" format="reference"/>
    </declare-styleable>
    <declare-styleable name="LoadingView">
        <attr name="leftColor" format="color"/>
        <attr name="middleColor" format="color"/>
        <attr name="rightColor" format="color"/>
        <attr name="timeforoneround" format="integer"/>
        <attr name="repeatCount" format="integer"/>
    </declare-styleable>
    <declare-styleable name="ImageWatcher">
        <attr name="maxLines" format="integer"/>
        <attr name="dividerWidth" format="dimension"/>
    </declare-styleable>
    <declare-styleable name="WLikeAnim">
        <attr name="wlduration" format="integer"/>
        <attr name="animOn" format="boolean"/>
        <attr name="ringWidth" format="dimension"/>
        <attr name="ringMaxWidth" format="dimension"/>
        <attr name="starPadding" format="dimension"/>
        <attr name="picWidth" format="dimension"/>
        <attr name="picHeight" format="dimension"/>
        <attr name="picScaledRate" format="float"/>
        <attr name="bigStarRadius" format="dimension"/>
        <attr name="smallStarRadius" format="dimension"/>
        <attr name="like" format="boolean"/>
        <attr name="starRotateDegree" format="integer"/>
        <attr name="smallStarRotateDegree" format="integer"/>
    </declare-styleable>
</resources>